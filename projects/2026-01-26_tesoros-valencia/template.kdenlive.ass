Dialogue: 0,00:00:00.33,00:00:00.90,Default,,0,0,0,,Hoy me he dado
Dialogue: 0,00:00:00.90,00:00:01.50,Default,,0,0,0,,una vuelta
Dialogue: 0,00:00:01.50,00:00:02.00,keyword,,0,0,0,,por Valencia
Dialogue: 0,00:00:01.100,00:00:03.27,Default,,0,0,0,,en bici...
Dialogue: 0,00:00:03.27,00:00:03.90,Default,,0,0,0,,y mirad
Dialogue: 0,00:00:03.90,00:00:04.37,Default,,0,0,0,,lo que me
Dialogue: 0,00:00:04.37,00:00:05.20,Default,,0,0,0,,he encontrado.
Dialogue: 0,00:00:05.20,00:00:06.40,keyword,,0,0,0,,Una planta
Dialogue: 0,00:00:06.40,00:00:07.27,keyword,,0,0,0,,Un sofá
Dialogue: 0,00:00:07.27,00:00:07.90,keyword,,0,0,0,,Y una maleta
Dialogue: 0,00:00:07.90,00:00:08.67,Default,,0,0,0,,antigua...
Dialogue: 0,00:00:08.67,00:00:09.93,Default,,0,0,0,,brutal
Dialogue: 0,00:00:09.93,00:00:10.67,Default,,0,0,0,,No están perfectas,
Dialogue: 0,00:00:10.67,00:00:11.53,keyword,,0,0,0,,pero funcionan.
Dialogue: 0,00:00:11.53,00:00:11.97,Default,,0,0,0,,Y podrían
Dialogue: 0,00:00:11.97,00:00:13.13,Default,,0,0,0,,haberse regalado
Dialogue: 0,00:00:13.13,00:00:13.37,Default,,0,0,0,,en vez de
Dialogue: 0,00:00:13.37,00:00:13.90,Default,,0,0,0,,acabar aquí
Dialogue: 0,00:00:13.90,00:00:14.80,keyword,,0,0,0,,tiradas.
Dialogue: 0,00:00:14.80,00:00:15.30,Default,,0,0,0,,Eso es lo que
Dialogue: 0,00:00:15.30,00:00:16.27,Default,,0,0,0,,me da pena.
Dialogue: 0,00:00:16.27,00:00:16.67,Default,,0,0,0,,Que alguien
Dialogue: 0,00:00:16.67,00:00:17.60,Default,,0,0,0,,las dejó aquí...
Dialogue: 0,00:00:17.60,00:00:18.60,Default,,0,0,0,,cuando alguien más
Dialogue: 0,00:00:18.60,00:00:19.10,Default,,0,0,0,,las podría
Dialogue: 0,00:00:19.10,00:00:20.07,Default,,0,0,0,,haber aprovechado.
Dialogue: 0,00:00:20.07,00:00:20.33,Default,,0,0,0,,Yo ya sé
Dialogue: 0,00:00:20.33,00:00:21.20,Default,,0,0,0,,qué hacer.
Dialogue: 0,00:00:21.20,00:00:22.30,Default,,0,0,0,,Saco el móvil,
Dialogue: 0,00:00:22.30,00:00:23.40,Default,,0,0,0,,abro Givore.
Dialogue: 0,00:00:23.40,00:00:23.57,Default,,0,0,0,,subo las fotos
Dialogue: 0,00:00:23.57,00:00:24.77,Default,,0,0,0,,con la ubicación...
Dialogue: 0,00:00:24.77,00:00:25.40,Default,,0,0,0,,y listo.
Dialogue: 0,00:00:25.40,00:00:25.90,Default,,0,0,0,,En un momento
Dialogue: 0,00:00:25.90,00:00:26.73,keyword,,0,0,0,,cualquiera del barrio
Dialogue: 0,00:00:26.73,00:00:27.57,Default,,0,0,0,,puede verlo.
Dialogue: 0,00:00:27.57,00:00:28.33,keyword,,0,0,0,,Así de simple.
Dialogue: 0,00:00:28.33,00:00:29.37,Default,,0,0,0,,Sin complicaciones.
Dialogue: 0,00:00:29.37,00:00:29.50,Default,,0,0,0,,Y mira,
Dialogue: 0,00:00:29.50,00:00:30.30,Default,,0,0,0,,esto ya no es
Dialogue: 0,00:00:30.30,00:00:30.60,Default,,0,0,0,,solo Valencia.
Dialogue: 0,00:00:30.60,00:00:32.60,keyword,,0,0,0,,Gijón y Alicante
Dialogue: 0,00:00:32.60,00:00:33.13,Default,,0,0,0,,también están
Dialogue: 0,00:00:33.13,00:00:33.57,Default,,0,0,0,,publicando.
Dialogue: 0,00:00:33.57,00:00:34.77,Default,,0,0,0,,La cosa crece.
Dialogue: 0,00:00:34.77,00:00:35.40,Default,,0,0,0,,Si aún no tenéis
Dialogue: 0,00:00:35.40,00:00:35.77,Default,,0,0,0,,la app...
Dialogue: 0,00:00:35.77,00:00:36.87,keyword,,0,0,0,,probadla.
Dialogue: 0,00:00:36.87,00:00:37.50,Default,,0,0,0,,La próxima vez
Dialogue: 0,00:00:37.50,00:00:38.10,Default,,0,0,0,,que tengáis algo
Dialogue: 0,00:00:38.10,00:00:39.00,Default,,0,0,0,,que ya no uséis,
Dialogue: 0,00:00:39.00,00:00:39.70,Default,,0,0,0,,compartidlo
Dialogue: 0,00:00:39.70,00:00:40.47,Default,,0,0,0,,antes de dejarlo
Dialogue: 0,00:00:40.47,00:00:40.93,Default,,0,0,0,,en la calle.
