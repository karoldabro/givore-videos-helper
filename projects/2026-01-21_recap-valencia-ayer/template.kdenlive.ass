Dialogue: 0,00:00:00.00,00:00:00.67,Default,,0,0,0,,Esto pasó
Dialogue: 0,00:00:00.67,00:00:01.87,Default,,0,0,0,,ayer en Valencia...
Dialogue: 0,00:00:01.87,00:00:02.97,Default,,0,0,0,,flipante.
Dialogue: 0,00:00:02.97,00:00:03.93,Default,,0,0,0,,Esperad a ver
Dialogue: 0,00:00:03.93,00:00:04.40,Default,,0,0,0,,esto.
Dialogue: 0,00:00:04.40,00:00:05.03,Default,,0,0,0,,Mirad.
Dialogue: 0,00:00:05.03,00:00:05.43,Default,,0,0,0,,Sillas,
Dialogue: 0,00:00:05.43,00:00:06.13,Default,,0,0,0,,un espejo,
Dialogue: 0,00:00:06.13,00:00:06.63,Default,,0,0,0,,un lavabo,
Dialogue: 0,00:00:06.63,00:00:07.20,Default,,0,0,0,,maletas...
Dialogue: 0,00:00:07.20,00:00:08.40,Default,,0,0,0,,Todo esto
Dialogue: 0,00:00:08.40,00:00:08.87,Default,,0,0,0,,lo subieron
Dialogue: 0,00:00:08.87,00:00:10.03,Default,,0,0,0,,vecinos de Valencia.
Dialogue: 0,00:00:10.03,00:00:10.67,Default,,0,0,0,,Ayer.
Dialogue: 0,00:00:10.67,00:00:11.87,Default,,0,0,0,,En un solo día.
Dialogue: 0,00:00:11.87,00:00:12.33,Default,,0,0,0,,Es que
Dialogue: 0,00:00:12.33,00:00:12.77,Default,,0,0,0,,me encanta.
Dialogue: 0,00:00:12.77,00:00:13.37,Default,,0,0,0,,Cosas que
Dialogue: 0,00:00:13.37,00:00:14.43,Default,,0,0,0,,podían haber acabado
Dialogue: 0,00:00:14.43,00:00:15.37,Default,,0,0,0,,en un contenedor...
Dialogue: 0,00:00:15.37,00:00:15.97,Default,,0,0,0,,ahora están
Dialogue: 0,00:00:15.97,00:00:16.40,Default,,0,0,0,,disponibles
Dialogue: 0,00:00:16.40,00:00:17.00,Default,,0,0,0,,para quien
Dialogue: 0,00:00:17.00,00:00:17.53,Default,,0,0,0,,las necesite.
Dialogue: 0,00:00:17.53,00:00:18.27,Default,,0,0,0,,Solo porque
Dialogue: 0,00:00:18.27,00:00:19.00,Default,,0,0,0,,alguien se tomó
Dialogue: 0,00:00:19.00,00:00:19.40,Default,,0,0,0,,un minuto
Dialogue: 0,00:00:19.40,00:00:20.43,Default,,0,0,0,,en compartirlas.
Dialogue: 0,00:00:20.43,00:00:21.03,Default,,0,0,0,,Y aquí es
Dialogue: 0,00:00:21.03,00:00:21.77,Default,,0,0,0,,donde entramos
Dialogue: 0,00:00:21.77,00:00:22.53,Default,,0,0,0,,todos.
Dialogue: 0,00:00:22.53,00:00:22.83,Default,,0,0,0,,Esto es
Dialogue: 0,00:00:22.83,00:00:23.63,Default,,0,0,0,,Givore.
Dialogue: 0,00:00:23.63,00:00:24.87,Default,,0,0,0,,Reciclaje social.
Dialogue: 0,00:00:24.87,00:00:25.93,Default,,0,0,0,,Vecinos compartiendo
Dialogue: 0,00:00:25.93,00:00:26.20,Default,,0,0,0,,lo que ya
Dialogue: 0,00:00:26.20,00:00:26.60,Default,,0,0,0,,no usan
Dialogue: 0,00:00:26.60,00:00:27.13,Default,,0,0,0,,para que otros
Dialogue: 0,00:00:27.13,00:00:27.93,Default,,0,0,0,,lo aprovechen.
Dialogue: 0,00:00:27.93,00:00:28.63,Default,,0,0,0,,Sin dinero.
Dialogue: 0,00:00:28.63,00:00:29.80,Default,,0,0,0,,Sin complicaciones.
Dialogue: 0,00:00:29.80,00:00:30.27,Default,,0,0,0,,Solo dar
Dialogue: 0,00:00:30.27,00:00:31.23,Default,,0,0,0,,y encontrar.
Dialogue: 0,00:00:31.23,00:00:31.60,Default,,0,0,0,,Y lo bonito
Dialogue: 0,00:00:31.60,00:00:31.93,Default,,0,0,0,,es que
Dialogue: 0,00:00:31.93,00:00:32.77,Default,,0,0,0,,cada publicación
Dialogue: 0,00:00:32.77,00:00:33.93,Default,,0,0,0,,es una conexión.
Dialogue: 0,00:00:33.93,00:00:34.93,Default,,0,0,0,,Alguien comparte,
Dialogue: 0,00:00:34.93,00:00:35.97,Default,,0,0,0,,alguien descubre.
Dialogue: 0,00:00:35.97,00:00:36.37,Default,,0,0,0,,Gente que
Dialogue: 0,00:00:36.37,00:00:37.37,Default,,0,0,0,,no se conoce,
Dialogue: 0,00:00:37.37,00:00:37.93,Default,,0,0,0,,ayudándose
Dialogue: 0,00:00:37.93,00:00:38.97,Default,,0,0,0,,sin saberlo.
Dialogue: 0,00:00:38.97,00:00:39.77,Default,,0,0,0,,Esto lo creamos
Dialogue: 0,00:00:39.77,00:00:40.37,Default,,0,0,0,,entre todos.
Dialogue: 0,00:00:40.37,00:00:41.07,Default,,0,0,0,,Cada cosa
Dialogue: 0,00:00:41.07,00:00:41.77,Default,,0,0,0,,que subís
Dialogue: 0,00:00:41.77,00:00:42.37,Default,,0,0,0,,es una menos
Dialogue: 0,00:00:42.37,00:00:43.20,Default,,0,0,0,,en la basura.
Dialogue: 0,00:00:43.20,00:00:44.00,Default,,0,0,0,,Y una más
Dialogue: 0,00:00:44.00,00:00:44.43,Default,,0,0,0,,en casa
Dialogue: 0,00:00:44.43,00:00:44.83,Default,,0,0,0,,de alguien
Dialogue: 0,00:00:44.83,00:00:45.73,Default,,0,0,0,,que la necesita.
Dialogue: 0,00:00:45.73,00:00:46.73,Default,,0,0,0,,Vamos a hacer
Dialogue: 0,00:00:46.73,00:00:47.53,Default,,0,0,0,,de Valencia
Dialogue: 0,00:00:47.53,00:00:48.03,Default,,0,0,0,,una ciudad
Dialogue: 0,00:00:48.03,00:00:48.77,Default,,0,0,0,,donde nada útil
Dialogue: 0,00:00:48.77,00:00:49.77,Default,,0,0,0,,se desperdicie.
