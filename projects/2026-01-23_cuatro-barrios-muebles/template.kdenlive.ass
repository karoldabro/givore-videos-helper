Dialogue: 0,00:00:00.00,00:00:01.20,Default,,0,0,0,,Cuatro barrios.
Dialogue: 0,00:00:01.20,00:00:01.80,Default,,0,0,0,,Una tarde.
Dialogue: 0,00:00:01.80,00:00:02.43,Default,,0,0,0,,Y mirad todo
Dialogue: 0,00:00:02.43,00:00:02.80,Default,,0,0,0,,lo que me
Dialogue: 0,00:00:02.80,00:00:03.43,Default,,0,0,0,,he encontrado.
Dialogue: 0,00:00:03.43,00:00:04.33,Default,,0,0,0,,Sillas de madera,
Dialogue: 0,00:00:04.33,00:00:05.03,Default,,0,0,0,,un cubito...
Dialogue: 0,00:00:05.03,00:00:05.67,Default,,0,0,0,,y en la app,
Dialogue: 0,00:00:05.67,00:00:06.33,Default,,0,0,0,,la comunidad
Dialogue: 0,00:00:06.33,00:00:07.20,Default,,0,0,0,,ha subido sofás,
Dialogue: 0,00:00:07.20,00:00:07.83,Default,,0,0,0,,cómodas...
Dialogue: 0,00:00:07.83,00:00:08.67,Default,,0,0,0,,de todo.
Dialogue: 0,00:00:08.67,00:00:09.40,Default,,0,0,0,,Y mola encontrar
Dialogue: 0,00:00:09.40,00:00:10.60,Default,,0,0,0,,cosas así, claro.
Dialogue: 0,00:00:10.60,00:00:11.13,Default,,0,0,0,,Pero lo mejor
Dialogue: 0,00:00:11.13,00:00:11.57,Default,,0,0,0,,sería que no
Dialogue: 0,00:00:11.57,00:00:12.57,Default,,0,0,0,,acabaran aquí.
Dialogue: 0,00:00:12.57,00:00:13.17,Default,,0,0,0,,Que los vecinos
Dialogue: 0,00:00:13.17,00:00:14.37,Default,,0,0,0,,las compartieran
Dialogue: 0,00:00:14.37,00:00:15.57,Default,,0,0,0,,antes de dejarlas
Dialogue: 0,00:00:15.57,00:00:16.57,Default,,0,0,0,,en la calle.
Dialogue: 0,00:00:16.57,00:00:17.53,Default,,0,0,0,,Y eso es
Dialogue: 0,00:00:17.53,00:00:18.13,Default,,0,0,0,,lo que hace
Dialogue: 0,00:00:18.13,00:00:18.47,Default,,0,0,0,,Givore.
Dialogue: 0,00:00:18.47,00:00:19.27,Default,,0,0,0,,Conectar a quien
Dialogue: 0,00:00:19.27,00:00:20.27,Default,,0,0,0,,tiene algo
Dialogue: 0,00:00:20.27,00:00:20.60,Default,,0,0,0,,con quien
Dialogue: 0,00:00:20.60,00:00:21.63,Default,,0,0,0,,lo necesita.
Dialogue: 0,00:00:21.63,00:00:22.33,Default,,0,0,0,,Sin que nada
Dialogue: 0,00:00:22.33,00:00:23.33,Default,,0,0,0,,acabe tirado.
Dialogue: 0,00:00:23.33,00:00:23.67,Default,,0,0,0,,Yo lo que hago
Dialogue: 0,00:00:23.67,00:00:24.77,Default,,0,0,0,,es subirlo.
Dialogue: 0,00:00:24.77,00:00:25.97,Default,,0,0,0,,Foto, título,
Dialogue: 0,00:00:25.97,00:00:27.00,Default,,0,0,0,,publico.
Dialogue: 0,00:00:27.00,00:00:27.47,Default,,0,0,0,,Y ya está
Dialogue: 0,00:00:27.47,00:00:27.87,Default,,0,0,0,,visible para
Dialogue: 0,00:00:27.87,00:00:28.77,Default,,0,0,0,,el barrio.
Dialogue: 0,00:00:28.77,00:00:29.33,Default,,0,0,0,,Pero lo bonito
Dialogue: 0,00:00:29.33,00:00:30.03,Default,,0,0,0,,es cuando alguien
Dialogue: 0,00:00:30.03,00:00:30.83,Default,,0,0,0,,comparte algo
Dialogue: 0,00:00:30.83,00:00:32.13,Default,,0,0,0,,antes de tirarlo.
Dialogue: 0,00:00:32.13,00:00:32.60,Default,,0,0,0,,Ahí es cuando
Dialogue: 0,00:00:32.60,00:00:33.93,Default,,0,0,0,,de verdad funciona.
Dialogue: 0,00:00:33.93,00:00:34.53,Default,,0,0,0,,¿Y vosotros?
Dialogue: 0,00:00:34.53,00:00:35.53,Default,,0,0,0,,¿Qué habéis visto
Dialogue: 0,00:00:35.53,00:00:35.97,Default,,0,0,0,,en la calle
Dialogue: 0,00:00:35.97,00:00:36.47,Default,,0,0,0,,últimamente?
Dialogue: 0,00:00:36.47,00:00:37.27,Default,,0,0,0,,¿Qué habéis
Dialogue: 0,00:00:37.27,00:00:38.23,Default,,0,0,0,,encontrado...
Dialogue: 0,00:00:38.23,00:00:39.23,Default,,0,0,0,,o qué tenéis
Dialogue: 0,00:00:39.23,00:00:39.80,Default,,0,0,0,,en casa que
Dialogue: 0,00:00:39.80,00:00:40.57,Default,,0,0,0,,ya no usáis?
Dialogue: 0,00:00:40.57,00:00:41.20,Default,,0,0,0,,Contádmelo en
Dialogue: 0,00:00:41.20,00:00:42.03,Default,,0,0,0,,comentarios.
