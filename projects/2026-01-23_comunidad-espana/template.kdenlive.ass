Dialogue: 0,00:00:00.17,00:00:01.40,Default,,0,0,0,,Esto empezó aquí...
Dialogue: 0,00:00:01.40,00:00:02.33,Default,,0,0,0,,en Valencia.
Dialogue: 0,00:00:02.33,00:00:03.20,Default,,0,0,0,,Pero fijaos
Dialogue: 0,00:00:03.20,00:00:03.77,Default,,0,0,0,,hasta dónde
Dialogue: 0,00:00:03.77,00:00:04.10,Default,,0,0,0,,ha llegado.
Dialogue: 0,00:00:04.10,00:00:04.77,Default,,0,0,0,,Madrid.
Dialogue: 0,00:00:04.77,00:00:05.60,Default,,0,0,0,,Gijón.
Dialogue: 0,00:00:05.60,00:00:06.37,Default,,0,0,0,,Cádiz.
Dialogue: 0,00:00:06.37,00:00:07.40,Default,,0,0,0,,Alicante.
Dialogue: 0,00:00:07.40,00:00:08.23,Default,,0,0,0,,Gente compartiendo
Dialogue: 0,00:00:08.23,00:00:08.60,Default,,0,0,0,,cosas en
Dialogue: 0,00:00:08.60,00:00:09.40,Default,,0,0,0,,toda España.
Dialogue: 0,00:00:09.40,00:00:10.30,Default,,0,0,0,,Es que
Dialogue: 0,00:00:10.30,00:00:10.77,Default,,0,0,0,,lo bonito
Dialogue: 0,00:00:10.77,00:00:11.73,Default,,0,0,0,,de esto...
Dialogue: 0,00:00:11.73,00:00:12.43,Default,,0,0,0,,es que
Dialogue: 0,00:00:12.43,00:00:13.53,Default,,0,0,0,,no tiene fronteras.
Dialogue: 0,00:00:13.53,00:00:13.93,Default,,0,0,0,,Alguien en
Dialogue: 0,00:00:13.93,00:00:14.60,Default,,0,0,0,,una ciudad
Dialogue: 0,00:00:14.60,00:00:15.57,Default,,0,0,0,,comparte algo.
Dialogue: 0,00:00:15.57,00:00:16.33,Default,,0,0,0,,Alguien en otra
Dialogue: 0,00:00:16.33,00:00:17.13,Default,,0,0,0,,lo encuentra.
Dialogue: 0,00:00:17.13,00:00:17.53,Default,,0,0,0,,Vecinos que
Dialogue: 0,00:00:17.53,00:00:18.60,Default,,0,0,0,,no se conocen...
Dialogue: 0,00:00:18.60,00:00:19.73,Default,,0,0,0,,conectando.
Dialogue: 0,00:00:19.73,00:00:20.70,Default,,0,0,0,,Lo que intentamos
Dialogue: 0,00:00:20.70,00:00:21.10,Default,,0,0,0,,es crear
Dialogue: 0,00:00:21.10,00:00:21.63,Default,,0,0,0,,comunidades
Dialogue: 0,00:00:21.63,00:00:22.37,Default,,0,0,0,,por toda España.
Dialogue: 0,00:00:22.37,00:00:23.30,Default,,0,0,0,,Que la gente
Dialogue: 0,00:00:23.30,00:00:24.33,Default,,0,0,0,,intercambie cosas
Dialogue: 0,00:00:24.33,00:00:24.70,Default,,0,0,0,,en vez de
Dialogue: 0,00:00:24.70,00:00:25.60,Default,,0,0,0,,tirarlas.
Dialogue: 0,00:00:25.60,00:00:26.80,Default,,0,0,0,,Givore ya está
Dialogue: 0,00:00:26.80,00:00:28.03,Default,,0,0,0,,en más sitios.
Dialogue: 0,00:00:28.03,00:00:28.63,Default,,0,0,0,,Y cada cosa
Dialogue: 0,00:00:28.63,00:00:28.97,Default,,0,0,0,,que alguien
Dialogue: 0,00:00:28.97,00:00:29.83,Default,,0,0,0,,comparte...
Dialogue: 0,00:00:29.83,00:00:30.70,Default,,0,0,0,,es una oportunidad
Dialogue: 0,00:00:30.70,00:00:31.53,Default,,0,0,0,,para otro.
Dialogue: 0,00:00:31.53,00:00:31.93,Default,,0,0,0,,Mirad lo que
Dialogue: 0,00:00:31.93,00:00:32.70,Default,,0,0,0,,están publicando.
Dialogue: 0,00:00:32.70,00:00:33.77,Default,,0,0,0,,Cosas que antes
Dialogue: 0,00:00:33.77,00:00:34.53,Default,,0,0,0,,se perdían...
Dialogue: 0,00:00:34.53,00:00:35.17,Default,,0,0,0,,ahora encuentran
Dialogue: 0,00:00:35.17,00:00:35.30,Default,,0,0,0,,a quien
Dialogue: 0,00:00:35.30,00:00:36.37,Default,,0,0,0,,las necesita.
Dialogue: 0,00:00:36.37,00:00:36.63,Default,,0,0,0,,Y oye,
Dialogue: 0,00:00:36.63,00:00:37.33,Default,,0,0,0,,gracias a todos
Dialogue: 0,00:00:37.33,00:00:37.83,Default,,0,0,0,,los que estáis
Dialogue: 0,00:00:37.83,00:00:38.93,Default,,0,0,0,,subiendo cosas.
Dialogue: 0,00:00:38.93,00:00:39.53,Default,,0,0,0,,Esto funciona
Dialogue: 0,00:00:39.53,00:00:40.73,Default,,0,0,0,,porque participáis.
Dialogue: 0,00:00:40.73,00:00:41.73,Default,,0,0,0,,Seguid así.
Dialogue: 0,00:00:41.73,00:00:42.33,Default,,0,0,0,,Si queréis
Dialogue: 0,00:00:42.33,00:00:42.50,Default,,0,0,0,,ser parte
Dialogue: 0,00:00:42.50,00:00:43.40,Default,,0,0,0,,de esto...
Dialogue: 0,00:00:43.40,00:00:43.80,Default,,0,0,0,,Givore.
Dialogue: 0,00:00:43.80,00:00:44.80,Default,,0,0,0,,Compartid
Dialogue: 0,00:00:44.80,00:00:45.20,Default,,0,0,0,,lo que ya
Dialogue: 0,00:00:45.20,00:00:45.90,Default,,0,0,0,,no uséis.
Dialogue: 0,00:00:45.90,00:00:46.13,Default,,0,0,0,,Encontrad
Dialogue: 0,00:00:46.13,00:00:47.33,Default,,0,0,0,,lo que necesitéis.
Dialogue: 0,00:00:47.33,00:00:48.30,Default,,0,0,0,,Así de fácil.
