Dialogue: 0,00:00:00.00,00:00:00.77,keyword,,0,0,0,,Un ventilador
Dialogue: 0,00:00:00.77,00:00:01.33,Default,,0,0,0,,de torre
Dialogue: 0,00:00:01.33,00:00:02.33,keyword,,0,0,0,,y un escritorio
Dialogue: 0,00:00:02.33,00:00:02.57,Default,,0,0,0,,de oficina...
Dialogue: 0,00:00:02.57,00:00:02.97,Default,,0,0,0,,tirados en
Dialogue: 0,00:00:02.97,00:00:04.33,keyword,,0,0,0,,el Grau.
Dialogue: 0,00:00:04.33,00:00:05.43,Default,,0,0,0,,Mirad el estado.
Dialogue: 0,00:00:05.43,00:00:05.63,Default,,0,0,0,,Es que
Dialogue: 0,00:00:05.63,00:00:06.00,Default,,0,0,0,,cada vez
Dialogue: 0,00:00:06.00,00:00:06.40,Default,,0,0,0,,que salgo
Dialogue: 0,00:00:06.40,00:00:07.37,keyword,,0,0,0,,por Valencia
Dialogue: 0,00:00:07.37,00:00:07.57,Default,,0,0,0,,en bici,
Dialogue: 0,00:00:07.57,00:00:08.00,Default,,0,0,0,,me encuentro
Dialogue: 0,00:00:08.00,00:00:08.57,Default,,0,0,0,,cosas así.
Dialogue: 0,00:00:08.57,00:00:09.33,Default,,0,0,0,,Cosas que
Dialogue: 0,00:00:09.33,00:00:10.60,Default,,0,0,0,,parecen estar bien...
Dialogue: 0,00:00:10.60,00:00:11.07,Default,,0,0,0,,ahí,
Dialogue: 0,00:00:11.07,00:00:11.93,Default,,0,0,0,,en la acera,
Dialogue: 0,00:00:11.93,00:00:12.93,Default,,0,0,0,,esperando.
Dialogue: 0,00:00:12.93,00:00:13.47,Default,,0,0,0,,Y lo peor
Dialogue: 0,00:00:13.47,00:00:14.67,Default,,0,0,0,,es que alguien,
Dialogue: 0,00:00:14.67,00:00:15.13,Default,,0,0,0,,a dos calles
Dialogue: 0,00:00:15.13,00:00:15.60,Default,,0,0,0,,de aquí,
Dialogue: 0,00:00:15.60,00:00:16.20,Default,,0,0,0,,puede que lleve
Dialogue: 0,00:00:16.20,00:00:17.00,Default,,0,0,0,,semanas buscando
Dialogue: 0,00:00:17.00,00:00:18.57,keyword,,0,0,0,,justo un escritorio
Dialogue: 0,00:00:18.57,00:00:18.93,Default,,0,0,0,,así.
Dialogue: 0,00:00:18.93,00:00:19.93,Default,,0,0,0,,Y no lo sabe.
Dialogue: 0,00:00:19.93,00:00:20.33,Default,,0,0,0,,¿Y sabéis
Dialogue: 0,00:00:20.33,00:00:20.67,Default,,0,0,0,,qué hago yo
Dialogue: 0,00:00:20.67,00:00:21.23,Default,,0,0,0,,cuando veo
Dialogue: 0,00:00:21.23,00:00:21.53,Default,,0,0,0,,algo así?
Dialogue: 0,00:00:21.53,00:00:21.83,Default,,0,0,0,,Lo subo
Dialogue: 0,00:00:21.83,00:00:22.87,keyword,,0,0,0,,a Givore.
Dialogue: 0,00:00:22.87,00:00:22.97,Default,,0,0,0,,Foto,
Dialogue: 0,00:00:22.97,00:00:23.43,Default,,0,0,0,,título,
Dialogue: 0,00:00:23.43,00:00:24.33,Default,,0,0,0,,publico.
Dialogue: 0,00:00:24.33,00:00:25.03,Default,,0,0,0,,Ya está visible
Dialogue: 0,00:00:25.03,00:00:25.53,Default,,0,0,0,,para todo
Dialogue: 0,00:00:25.53,00:00:26.07,Default,,0,0,0,,el barrio.
Dialogue: 0,00:00:26.07,00:00:27.17,Default,,0,0,0,,Así de simple.
Dialogue: 0,00:00:27.17,00:00:27.77,keyword,,0,0,0,,El escritorio
Dialogue: 0,00:00:27.77,00:00:28.67,Default,,0,0,0,,tiene buena pinta.
Dialogue: 0,00:00:28.67,00:00:30.47,keyword,,0,0,0,,El ventilador...
Dialogue: 0,00:00:30.47,00:00:31.23,Default,,0,0,0,,habrá que probarlo.
Dialogue: 0,00:00:31.23,00:00:32.17,Default,,0,0,0,,Pero alguien cerca
Dialogue: 0,00:00:32.17,00:00:32.67,Default,,0,0,0,,puede darles
Dialogue: 0,00:00:32.67,00:00:33.40,Default,,0,0,0,,una oportunidad.
Dialogue: 0,00:00:33.40,00:00:34.07,Default,,0,0,0,,Y oye...
Dialogue: 0,00:00:34.07,00:00:34.80,Default,,0,0,0,,si tenéis algo
Dialogue: 0,00:00:34.80,00:00:35.07,Default,,0,0,0,,en casa
Dialogue: 0,00:00:35.07,00:00:36.17,Default,,0,0,0,,que ya no usáis,
Dialogue: 0,00:00:36.17,00:00:36.47,Default,,0,0,0,,subidlo
Dialogue: 0,00:00:36.47,00:00:37.47,Default,,0,0,0,,antes de tirarlo.
Dialogue: 0,00:00:37.47,00:00:38.00,Default,,0,0,0,,Lo que para ti
Dialogue: 0,00:00:38.00,00:00:38.47,Default,,0,0,0,,sobra,
Dialogue: 0,00:00:38.47,00:00:38.80,Default,,0,0,0,,para otro
Dialogue: 0,00:00:38.80,00:00:39.17,Default,,0,0,0,,es justo
Dialogue: 0,00:00:39.17,00:00:40.07,Default,,0,0,0,,lo que busca.
Dialogue: 0,00:00:40.07,00:00:40.53,Default,,0,0,0,,Imaginad
Dialogue: 0,00:00:40.53,00:00:41.00,Default,,0,0,0,,si cada vecino
Dialogue: 0,00:00:41.00,00:00:42.00,keyword,,0,0,0,,compartiera
Dialogue: 0,00:00:42.00,00:00:42.73,Default,,0,0,0,,en vez de tirar.
Dialogue: 0,00:00:42.73,00:00:43.93,Default,,0,0,0,,Nada acabaría así.
