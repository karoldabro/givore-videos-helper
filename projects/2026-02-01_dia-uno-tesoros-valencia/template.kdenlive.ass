Dialogue: 0,00:00:00.00,00:00:00.83,Default,,0,0,0,,Dia uno...
Dialogue: 0,00:00:00.83,00:00:01.80,Default,,0,0,0,,recorriendo la ciudad
Dialogue: 0,00:00:01.80,00:00:02.07,Default,,0,0,0,,en bici
Dialogue: 0,00:00:02.07,00:00:03.07,Default,,0,0,0,,para demostrar algo.
Dialogue: 0,00:00:03.07,00:00:03.87,Default,,0,0,0,,Que las calles
Dialogue: 0,00:00:03.87,00:00:04.20,Default,,0,0,0,,estan llenas
Dialogue: 0,00:00:04.20,00:00:04.47,Default,,0,0,0,,de cosas
Dialogue: 0,00:00:04.47,00:00:05.13,Default,,0,0,0,,que merecen
Dialogue: 0,00:00:05.13,00:00:06.33,Default,,0,0,0,,otra oportunidad.
Dialogue: 0,00:00:06.33,00:00:06.87,Default,,0,0,0,,Vamos a verlo.
Dialogue: 0,00:00:06.87,00:00:08.00,Default,,0,0,0,,Primera parada
Dialogue: 0,00:00:08.00,00:00:08.47,Default,,0,0,0,,y...
Dialogue: 0,00:00:08.47,00:00:09.67,Default,,0,0,0,,una estanteria.
Dialogue: 0,00:00:09.67,00:00:10.60,Default,,0,0,0,,Alguien la saco
Dialogue: 0,00:00:10.60,00:00:12.00,Default,,0,0,0,,y ahi se quedo.
Dialogue: 0,00:00:12.00,00:00:13.27,Default,,0,0,0,,Pero fijaos...
Dialogue: 0,00:00:13.27,00:00:13.97,Default,,0,0,0,,esto no tendria
Dialogue: 0,00:00:13.97,00:00:14.93,Default,,0,0,0,,por que acabar asi.
Dialogue: 0,00:00:14.93,00:00:15.37,Default,,0,0,0,,Si antes
Dialogue: 0,00:00:15.37,00:00:15.87,Default,,0,0,0,,de sacarla
Dialogue: 0,00:00:15.87,00:00:16.83,Default,,0,0,0,,a la calle,
Dialogue: 0,00:00:16.83,00:00:18.23,Default,,0,0,0,,la hubiera compartido...
Dialogue: 0,00:00:18.23,00:00:18.93,Default,,0,0,0,,ahora mismo
Dialogue: 0,00:00:18.93,00:00:19.60,Default,,0,0,0,,estaria en casa
Dialogue: 0,00:00:19.60,00:00:20.03,Default,,0,0,0,,de alguien
Dialogue: 0,00:00:20.03,00:00:21.20,Default,,0,0,0,,que la necesita.
Dialogue: 0,00:00:21.20,00:00:21.63,Default,,0,0,0,,Y eso
Dialogue: 0,00:00:21.63,00:00:22.27,Default,,0,0,0,,no es solo
Dialogue: 0,00:00:22.27,00:00:23.00,Default,,0,0,0,,mi opinion.
Dialogue: 0,00:00:23.00,00:00:23.57,Default,,0,0,0,,Mirad lo que
Dialogue: 0,00:00:23.57,00:00:24.53,Default,,0,0,0,,ha pasado hoy
Dialogue: 0,00:00:24.53,00:00:25.23,Default,,0,0,0,,en la app.
Dialogue: 0,00:00:25.23,00:00:25.87,Default,,0,0,0,,Gente real
Dialogue: 0,00:00:25.87,00:00:26.40,Default,,0,0,0,,compartiendo
Dialogue: 0,00:00:26.40,00:00:27.43,Default,,0,0,0,,antes de tirar.
Dialogue: 0,00:00:27.43,00:00:27.87,Default,,0,0,0,,Muebles,
Dialogue: 0,00:00:27.87,00:00:28.00,Default,,0,0,0,,ropa,
Dialogue: 0,00:00:28.00,00:00:28.57,Default,,0,0,0,,objetos...
Dialogue: 0,00:00:28.57,00:00:29.57,Default,,0,0,0,,dandoles salida
Dialogue: 0,00:00:29.57,00:00:30.37,Default,,0,0,0,,en vez de dejarlos
Dialogue: 0,00:00:30.37,00:00:31.07,Default,,0,0,0,,en la acera.
Dialogue: 0,00:00:31.07,00:00:31.47,Default,,0,0,0,,Ahi esta
Dialogue: 0,00:00:31.47,00:00:32.23,Default,,0,0,0,,la diferencia.
Dialogue: 0,00:00:32.23,00:00:32.77,Default,,0,0,0,,Dar en vez
Dialogue: 0,00:00:32.77,00:00:33.53,Default,,0,0,0,,de tirar.
Dialogue: 0,00:00:33.53,00:00:34.03,Default,,0,0,0,,Y hacerlo
Dialogue: 0,00:00:34.03,00:00:35.13,Default,,0,0,0,,es ridiculamente
Dialogue: 0,00:00:35.13,00:00:35.80,Default,,0,0,0,,sencillo.
Dialogue: 0,00:00:35.80,00:00:36.43,Default,,0,0,0,,Abro Givore,
Dialogue: 0,00:00:36.43,00:00:37.07,Default,,0,0,0,,subo una foto,
Dialogue: 0,00:00:37.07,00:00:38.13,Default,,0,0,0,,le pongo nombre...
Dialogue: 0,00:00:38.13,00:00:39.00,Default,,0,0,0,,y listo.
Dialogue: 0,00:00:39.00,00:00:39.33,Default,,0,0,0,,Esto es
Dialogue: 0,00:00:39.33,00:00:40.47,Default,,0,0,0,,el dia uno.
Dialogue: 0,00:00:40.47,00:00:41.13,Default,,0,0,0,,Y ya hay pruebas
Dialogue: 0,00:00:41.13,00:00:41.80,Default,,0,0,0,,de que funciona.
Dialogue: 0,00:00:41.80,00:00:42.30,Default,,0,0,0,,Que hay
Dialogue: 0,00:00:42.30,00:00:42.93,Default,,0,0,0,,en vuestro barrio
Dialogue: 0,00:00:42.93,00:00:43.27,Default,,0,0,0,,que merece
Dialogue: 0,00:00:43.27,00:00:43.77,Default,,0,0,0,,otra vida?
Dialogue: 0,00:00:43.77,00:00:44.90,Default,,0,0,0,,Que teneis en casa
Dialogue: 0,00:00:44.90,00:00:45.27,Default,,0,0,0,,que alguien
Dialogue: 0,00:00:45.27,00:00:45.80,Default,,0,0,0,,aprovecharia?
Dialogue: 0,00:00:45.80,00:00:47.07,Default,,0,0,0,,Contadmelo abajo.
Dialogue: 0,00:00:47.07,00:00:47.57,Default,,0,0,0,,Quiero ver
Dialogue: 0,00:00:47.57,00:00:48.40,Default,,0,0,0,,lo que descubris.
