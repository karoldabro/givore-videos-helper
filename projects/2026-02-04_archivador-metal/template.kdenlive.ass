Dialogue: 0,00:00:00.00,00:00:00.57,Default,,0,0,0,,Hay un objeto
Dialogue: 0,00:00:00.57,00:00:01.40,Default,,0,0,0,,que casi nadie
Dialogue: 0,00:00:01.40,00:00:02.17,Default,,0,0,0,,recoge...
Dialogue: 0,00:00:02.17,00:00:02.63,Default,,0,0,0,,y es uno
Dialogue: 0,00:00:02.63,00:00:03.40,Default,,0,0,0,,de los mas utiles
Dialogue: 0,00:00:03.40,00:00:04.07,Default,,0,0,0,,que existen.
Dialogue: 0,00:00:04.07,00:00:04.77,Default,,0,0,0,,Un archivador
Dialogue: 0,00:00:04.77,00:00:05.60,Default,,0,0,0,,de oficina.
Dialogue: 0,00:00:05.60,00:00:06.43,Default,,0,0,0,,De metal.
Dialogue: 0,00:00:06.43,00:00:07.17,Default,,0,0,0,,Entero.
Dialogue: 0,00:00:07.17,00:00:07.77,Default,,0,0,0,,Funcional.
Dialogue: 0,00:00:07.77,00:00:08.67,Default,,0,0,0,,Tirado junto
Dialogue: 0,00:00:08.67,00:00:09.97,Default,,0,0,0,,a un contenedor.
Dialogue: 0,00:00:09.97,00:00:10.47,Default,,0,0,0,,Este tipo
Dialogue: 0,00:00:10.47,00:00:11.27,Default,,0,0,0,,de mueble,
Dialogue: 0,00:00:11.27,00:00:11.73,Default,,0,0,0,,nuevo,
Dialogue: 0,00:00:11.73,00:00:13.03,Default,,0,0,0,,cuesta entre
Dialogue: 0,00:00:13.03,00:00:14.47,Default,,0,0,0,,sesenta y cien euros.
Dialogue: 0,00:00:14.47,00:00:15.33,Default,,0,0,0,,Y aqui esta...
Dialogue: 0,00:00:15.33,00:00:16.20,Default,,0,0,0,,esperando a que pase
Dialogue: 0,00:00:16.20,00:00:16.53,Default,,0,0,0,,el camion
Dialogue: 0,00:00:16.53,00:00:17.13,Default,,0,0,0,,de la basura.
Dialogue: 0,00:00:17.13,00:00:18.47,Default,,0,0,0,,Pasa constantemente.
Dialogue: 0,00:00:18.47,00:00:19.27,Default,,0,0,0,,Objetos que valen,
Dialogue: 0,00:00:19.27,00:00:20.37,Default,,0,0,0,,que funcionan...
Dialogue: 0,00:00:20.37,00:00:21.07,Default,,0,0,0,,y se pierden
Dialogue: 0,00:00:21.07,00:00:22.20,Default,,0,0,0,,porque no hay forma
Dialogue: 0,00:00:22.20,00:00:23.03,Default,,0,0,0,,de darles salida
Dialogue: 0,00:00:23.03,00:00:23.40,Default,,0,0,0,,a tiempo.
Dialogue: 0,00:00:23.40,00:00:24.33,Default,,0,0,0,,Pero eso
Dialogue: 0,00:00:24.33,00:00:25.17,Default,,0,0,0,,tiene solucion.
Dialogue: 0,00:00:25.17,00:00:25.97,Default,,0,0,0,,Y es mas simple
Dialogue: 0,00:00:25.97,00:00:27.17,Default,,0,0,0,,de lo que pensais.
Dialogue: 0,00:00:27.17,00:00:27.80,Default,,0,0,0,,En Givore,
Dialogue: 0,00:00:27.80,00:00:28.87,Default,,0,0,0,,la gente comparte
Dialogue: 0,00:00:28.87,00:00:29.17,Default,,0,0,0,,lo que ya
Dialogue: 0,00:00:29.17,00:00:29.93,Default,,0,0,0,,no necesita
Dialogue: 0,00:00:29.93,00:00:30.77,Default,,0,0,0,,antes de tirarlo.
Dialogue: 0,00:00:30.77,00:00:32.07,Default,,0,0,0,,Una foto,
Dialogue: 0,00:00:32.07,00:00:33.37,Default,,0,0,0,,un titulo...
Dialogue: 0,00:00:33.37,00:00:34.07,Default,,0,0,0,,y queda visible
Dialogue: 0,00:00:34.07,00:00:34.40,Default,,0,0,0,,para quien
Dialogue: 0,00:00:34.40,00:00:35.20,Default,,0,0,0,,lo busque.
Dialogue: 0,00:00:35.20,00:00:35.83,Default,,0,0,0,,Sin dinero.
Dialogue: 0,00:00:35.83,00:00:37.60,Default,,0,0,0,,Sin intermediarios.
Dialogue: 0,00:00:37.60,00:00:38.53,Default,,0,0,0,,Este archivador,
Dialogue: 0,00:00:38.53,00:00:39.60,Default,,0,0,0,,por ejemplo,
Dialogue: 0,00:00:39.60,00:00:40.83,Default,,0,0,0,,podria haber acabado
Dialogue: 0,00:00:40.83,00:00:41.87,Default,,0,0,0,,en una asociacion,
Dialogue: 0,00:00:41.87,00:00:42.57,Default,,0,0,0,,en una oficina
Dialogue: 0,00:00:42.57,00:00:42.93,Default,,0,0,0,,de barrio,
Dialogue: 0,00:00:42.93,00:00:43.73,Default,,0,0,0,,en casa de alguien
Dialogue: 0,00:00:43.73,00:00:44.23,Default,,0,0,0,,que lo estaba
Dialogue: 0,00:00:44.23,00:00:45.17,Default,,0,0,0,,buscando.
Dialogue: 0,00:00:45.17,00:00:46.27,Default,,0,0,0,,En vez de eso...
Dialogue: 0,00:00:46.27,00:00:47.00,Default,,0,0,0,,basura.
Dialogue: 0,00:00:47.00,00:00:47.93,Default,,0,0,0,,Guardad este video.
Dialogue: 0,00:00:47.93,00:00:48.83,Default,,0,0,0,,Para cuando veais
Dialogue: 0,00:00:48.83,00:00:49.57,Default,,0,0,0,,algo util
Dialogue: 0,00:00:49.57,00:00:50.00,Default,,0,0,0,,en la calle
Dialogue: 0,00:00:50.00,00:00:50.57,Default,,0,0,0,,y no sepais
Dialogue: 0,00:00:50.57,00:00:51.33,Default,,0,0,0,,que hacer.
Dialogue: 0,00:00:51.33,00:00:51.67,Default,,0,0,0,,Givore.
Dialogue: 0,00:00:51.67,00:00:52.37,Default,,0,0,0,,Compartir
Dialogue: 0,00:00:52.37,00:00:53.27,Default,,0,0,0,,antes de tirar.
