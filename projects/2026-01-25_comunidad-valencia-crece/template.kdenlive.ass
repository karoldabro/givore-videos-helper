Dialogue: 0,00:00:00.00,00:00:00.73,Default,,0,0,0,,Algo está pasando
Dialogue: 0,00:00:00.73,00:00:01.67,Default,,0,0,0,,en Valencia...
Dialogue: 0,00:00:01.67,00:00:02.03,Default,,0,0,0,,y tengo que
Dialogue: 0,00:00:02.03,00:00:03.20,Default,,0,0,0,,enseñároslo.
Dialogue: 0,00:00:03.20,00:00:03.73,Default,,0,0,0,,Quedaos hasta
Dialogue: 0,00:00:03.73,00:00:04.17,Default,,0,0,0,,el final
Dialogue: 0,00:00:04.17,00:00:04.53,Default,,0,0,0,,porque esto
Dialogue: 0,00:00:04.53,00:00:05.47,Default,,0,0,0,,os va a gustar.
Dialogue: 0,00:00:05.47,00:00:06.00,Default,,0,0,0,,Fijaos.
Dialogue: 0,00:00:06.00,00:00:07.20,Default,,0,0,0,,Cada día aparecen
Dialogue: 0,00:00:07.20,00:00:08.07,Default,,0,0,0,,publicaciones nuevas
Dialogue: 0,00:00:08.07,00:00:08.47,Default,,0,0,0,,en barrios
Dialogue: 0,00:00:08.47,00:00:09.53,Default,,0,0,0,,de Valencia...
Dialogue: 0,00:00:09.53,00:00:10.17,Default,,0,0,0,,Ruzafa,
Dialogue: 0,00:00:10.17,00:00:10.80,Default,,0,0,0,,Benimaclet,
Dialogue: 0,00:00:10.80,00:00:11.53,Default,,0,0,0,,el Cabanyal,
Dialogue: 0,00:00:11.53,00:00:12.17,Default,,0,0,0,,Patraix,
Dialogue: 0,00:00:12.17,00:00:12.77,Default,,0,0,0,,La Zaidía,
Dialogue: 0,00:00:12.77,00:00:13.40,Default,,0,0,0,,Torrefiel,
Dialogue: 0,00:00:13.40,00:00:14.33,Default,,0,0,0,,Benicalap...
Dialogue: 0,00:00:14.33,00:00:14.73,Default,,0,0,0,,incluso en
Dialogue: 0,00:00:14.73,00:00:15.43,Default,,0,0,0,,Torrent.
Dialogue: 0,00:00:15.43,00:00:15.87,Default,,0,0,0,,Gente que
Dialogue: 0,00:00:15.87,00:00:17.00,Default,,0,0,0,,antes de tirar
Dialogue: 0,00:00:17.00,00:00:17.07,Default,,0,0,0,,algo,
Dialogue: 0,00:00:17.07,00:00:17.53,Default,,0,0,0,,lo comparte.
Dialogue: 0,00:00:17.53,00:00:18.13,Default,,0,0,0,,Y lo bonito
Dialogue: 0,00:00:18.13,00:00:18.47,Default,,0,0,0,,es que
Dialogue: 0,00:00:18.47,00:00:19.33,Default,,0,0,0,,no se conocen
Dialogue: 0,00:00:19.33,00:00:20.33,Default,,0,0,0,,entre ellos.
Dialogue: 0,00:00:20.33,00:00:20.83,Default,,0,0,0,,Son vecinos
Dialogue: 0,00:00:20.83,00:00:21.60,Default,,0,0,0,,de dos calles
Dialogue: 0,00:00:21.60,00:00:22.37,Default,,0,0,0,,más allá...
Dialogue: 0,00:00:22.37,00:00:22.87,Default,,0,0,0,,conectando
Dialogue: 0,00:00:22.87,00:00:23.63,Default,,0,0,0,,sin saberlo.
Dialogue: 0,00:00:23.63,00:00:24.27,Default,,0,0,0,,Alguien publica
Dialogue: 0,00:00:24.27,00:00:25.00,Default,,0,0,0,,una silla.
Dialogue: 0,00:00:25.00,00:00:25.93,Default,,0,0,0,,Alguien la ve.
Dialogue: 0,00:00:25.93,00:00:27.03,Default,,0,0,0,,Y esa silla
Dialogue: 0,00:00:27.03,00:00:27.53,Default,,0,0,0,,ya no acaba
Dialogue: 0,00:00:27.53,00:00:28.07,Default,,0,0,0,,en la basura.
Dialogue: 0,00:00:28.07,00:00:28.57,Default,,0,0,0,,Y aquí es
Dialogue: 0,00:00:28.57,00:00:29.03,Default,,0,0,0,,donde entramos
Dialogue: 0,00:00:29.03,00:00:29.37,Default,,0,0,0,,todos.
Dialogue: 0,00:00:29.37,00:00:30.00,Default,,0,0,0,,Mira.
Dialogue: 0,00:00:30.00,00:00:30.83,Default,,0,0,0,,Abro Givore...
Dialogue: 0,00:00:30.83,00:00:31.80,Default,,0,0,0,,y esto es
Dialogue: 0,00:00:31.80,00:00:32.67,Default,,0,0,0,,lo que veo.
Dialogue: 0,00:00:32.67,00:00:33.33,Default,,0,0,0,,Publicaciones
Dialogue: 0,00:00:33.33,00:00:34.17,Default,,0,0,0,,reales.
Dialogue: 0,00:00:34.17,00:00:34.83,Default,,0,0,0,,De vecinos
Dialogue: 0,00:00:34.83,00:00:35.23,Default,,0,0,0,,reales.
Dialogue: 0,00:00:35.23,00:00:36.13,Default,,0,0,0,,Cada una
Dialogue: 0,00:00:36.13,00:00:37.33,Default,,0,0,0,,es una oportunidad
Dialogue: 0,00:00:37.33,00:00:37.60,Default,,0,0,0,,que antes
Dialogue: 0,00:00:37.60,00:00:38.07,Default,,0,0,0,,se perdía.
Dialogue: 0,00:00:38.07,00:00:38.97,Default,,0,0,0,,Esto lo creamos
Dialogue: 0,00:00:38.97,00:00:39.83,Default,,0,0,0,,entre todos.
Dialogue: 0,00:00:39.83,00:00:40.63,Default,,0,0,0,,Vecinos ayudando
Dialogue: 0,00:00:40.63,00:00:41.33,Default,,0,0,0,,a vecinos.
Dialogue: 0,00:00:41.33,00:00:41.97,Default,,0,0,0,,¿Y sabéis qué?
Dialogue: 0,00:00:41.97,00:00:42.43,Default,,0,0,0,,Cada vez
Dialogue: 0,00:00:42.43,00:00:43.40,Default,,0,0,0,,somos más.
Dialogue: 0,00:00:43.40,00:00:44.17,Default,,0,0,0,,Y eso...
Dialogue: 0,00:00:44.17,00:00:44.53,Default,,0,0,0,,eso es
Dialogue: 0,00:00:44.53,00:00:45.37,Default,,0,0,0,,lo que mola.
