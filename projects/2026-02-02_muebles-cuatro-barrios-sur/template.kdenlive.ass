Dialogue: 0,00:00:00.00,00:00:01.93,Default,,0,0,0,,Lo que os voy \Na enseñar hoy...
Dialogue: 0,00:00:01.93,00:00:03.47,Default,,0,0,0,,lleva semanas \Npasando.
Dialogue: 0,00:00:03.47,00:00:04.67,Default,,0,0,0,,Y muy poca \Ngente lo sabe.
Dialogue: 0,00:00:04.67,00:00:05.77,Default,,0,0,0,,Quedaos hasta \Nel final
Dialogue: 0,00:00:05.77,00:00:06.77,Default,,0,0,0,,porque vais \Na flipar.
Dialogue: 0,00:00:06.77,00:00:08.07,Default,,0,0,0,,Cuatro barrios.
Dialogue: 0,00:00:08.07,00:00:08.93,Default,,0,0,0,,La Raiosa,
Dialogue: 0,00:00:08.93,00:00:09.77,Default,,0,0,0,,Favara,
Dialogue: 0,00:00:09.77,00:00:10.93,Default,,0,0,0,,El Huerto \Nde Senabre,
Dialogue: 0,00:00:10.93,00:00:12.17,Default,,0,0,0,,La Cruz Cubierta.
Dialogue: 0,00:00:12.17,00:00:13.03,Default,,0,0,0,,Todos pegados.
Dialogue: 0,00:00:13.03,00:00:13.93,Default,,0,0,0,,Y en cada uno...
Dialogue: 0,00:00:13.93,00:00:14.53,Default,,0,0,0,,muebles,
Dialogue: 0,00:00:14.53,00:00:15.37,Default,,0,0,0,,electrodomésticos,
Dialogue: 0,00:00:15.37,00:00:16.43,Default,,0,0,0,,cosas útiles.
Dialogue: 0,00:00:16.43,00:00:17.77,Default,,0,0,0,,Ahí, \Nen la acera.
Dialogue: 0,00:00:17.77,00:00:19.23,Default,,0,0,0,,Sin que nadie \Nlos aproveche.
Dialogue: 0,00:00:19.23,00:00:19.97,Default,,0,0,0,,Y lo fuerte es
Dialogue: 0,00:00:19.97,00:00:20.80,Default,,0,0,0,,que a unas \Nmanzanas
Dialogue: 0,00:00:20.80,00:00:22.27,Default,,0,0,0,,de cada uno \Nde esos objetos...
Dialogue: 0,00:00:22.27,00:00:22.93,Default,,0,0,0,,vive alguien
Dialogue: 0,00:00:22.93,00:00:24.20,Default,,0,0,0,,que le vendría \Nperfecto.
Dialogue: 0,00:00:24.20,00:00:24.80,Default,,0,0,0,,Un vecino
Dialogue: 0,00:00:24.80,00:00:26.03,Default,,0,0,0,,montando su \Nprimer piso.
Dialogue: 0,00:00:26.03,00:00:28.47,Default,,0,0,0,,Alguien que justo \Nbusca una estantería.
Dialogue: 0,00:00:28.47,00:00:30.27,Default,,0,0,0,,Pero no \Nse enteran.
Dialogue: 0,00:00:30.27,00:00:31.00,Default,,0,0,0,,Y eso es
Dialogue: 0,00:00:31.00,00:00:32.60,Default,,0,0,0,,lo que podemos \Ncambiar.
Dialogue: 0,00:00:32.60,00:00:33.67,Default,,0,0,0,,Os enseño cómo.
Dialogue: 0,00:00:33.67,00:00:34.13,Default,,0,0,0,,En Givore,
Dialogue: 0,00:00:34.13,00:00:35.33,Default,,0,0,0,,la gente comparte
Dialogue: 0,00:00:35.33,00:00:36.03,Default,,0,0,0,,lo que ya \Nno usa
Dialogue: 0,00:00:36.03,00:00:37.13,Default,,0,0,0,,antes de tirarlo.
Dialogue: 0,00:00:37.13,00:00:38.17,Default,,0,0,0,,Muebles,
Dialogue: 0,00:00:38.17,00:00:38.97,Default,,0,0,0,,electrodomésticos,
Dialogue: 0,00:00:38.97,00:00:39.77,Default,,0,0,0,,lo que sea.
Dialogue: 0,00:00:39.77,00:00:40.73,Default,,0,0,0,,Lo suben,
Dialogue: 0,00:00:40.73,00:00:41.73,Default,,0,0,0,,y cualquier \Nvecino cerca
Dialogue: 0,00:00:41.73,00:00:42.07,Default,,0,0,0,,lo ve
Dialogue: 0,00:00:42.07,00:00:43.20,Default,,0,0,0,,y lo puede \Nrecoger.
Dialogue: 0,00:00:43.20,00:00:44.33,Default,,0,0,0,,Así de fácil.
Dialogue: 0,00:00:44.33,00:00:44.87,Default,,0,0,0,,Lo que yo \Nencuentro
Dialogue: 0,00:00:44.87,00:00:45.77,Default,,0,0,0,,en la calle
Dialogue: 0,00:00:45.77,00:00:46.83,Default,,0,0,0,,son solo ejemplos
Dialogue: 0,00:00:46.83,00:00:47.40,Default,,0,0,0,,de lo que \Nse pierde
Dialogue: 0,00:00:47.40,00:00:47.97,Default,,0,0,0,,cada día.
Dialogue: 0,00:00:47.97,00:00:49.47,Default,,0,0,0,,Cosas con vida \Npor delante
Dialogue: 0,00:00:49.47,00:00:50.67,Default,,0,0,0,,que acaban \Nen la basura...
Dialogue: 0,00:00:50.67,00:00:51.77,Default,,0,0,0,,cuando alguien
Dialogue: 0,00:00:51.77,00:00:52.63,Default,,0,0,0,,las habría \Nquerido.
Dialogue: 0,00:00:52.63,00:00:54.00,Default,,0,0,0,,Pero fijaos.
Dialogue: 0,00:00:54.00,00:00:54.80,Default,,0,0,0,,En la app,
Dialogue: 0,00:00:54.80,00:00:55.47,Default,,0,0,0,,vecinos ya están
Dialogue: 0,00:00:55.47,00:00:56.60,Default,,0,0,0,,publicando lo que \Nles sobra.
Dialogue: 0,00:00:56.60,00:00:57.07,Default,,0,0,0,,Estanterías,
Dialogue: 0,00:00:57.07,00:00:57.57,Default,,0,0,0,,microondas,
Dialogue: 0,00:00:57.57,00:00:58.47,Default,,0,0,0,,sillas...
Dialogue: 0,00:00:58.47,00:00:59.17,Default,,0,0,0,,disponibles para
Dialogue: 0,00:00:59.17,00:00:59.93,Default,,0,0,0,,quien las necesite.
Dialogue: 0,00:00:59.93,00:01:01.43,Default,,0,0,0,,Sin que lleguen \Na la acera.
Dialogue: 0,00:01:01.43,00:01:02.07,Default,,0,0,0,,Esa es la idea.
Dialogue: 0,00:01:02.07,00:01:03.87,Default,,0,0,0,,Que las cosas \Nse compartan
Dialogue: 0,00:01:03.87,00:01:05.53,Default,,0,0,0,,antes de acabar \Ntiradas.
Dialogue: 0,00:01:05.53,00:01:05.93,Default,,0,0,0,,Y cada vez
Dialogue: 0,00:01:05.93,00:01:06.87,Default,,0,0,0,,somos más \Nhaciéndolo.
Dialogue: 0,00:01:06.87,00:01:08.03,Default,,0,0,0,,¿Tenéis algo \Nen casa
Dialogue: 0,00:01:08.03,00:01:08.83,Default,,0,0,0,,que ya \Nno usáis?
Dialogue: 0,00:01:08.83,00:01:10.10,Default,,0,0,0,,Contádmelo abajo.
