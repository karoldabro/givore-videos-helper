Dialogue: 0,00:00:00.73,00:00:02.10,Default,,0,0,0,,Valencia está llena
Dialogue: 0,00:00:02.10,00:00:02.47,Default,,0,0,0,,de cosas
Dialogue: 0,00:00:02.47,00:00:02.77,Default,,0,0,0,,esperando a
Dialogue: 0,00:00:02.77,00:00:03.100,Default,,0,0,0,,ser descubiertas.
Dialogue: 0,00:00:04.00,00:00:04.47,Default,,0,0,0,,Esta semana
Dialogue: 0,00:00:04.47,00:00:05.27,Default,,0,0,0,,he salido en bici
Dialogue: 0,00:00:05.27,00:00:06.47,Default,,0,0,0,,por media ciudad...
Dialogue: 0,00:00:06.47,00:00:07.10,Default,,0,0,0,,y cada barrio
Dialogue: 0,00:00:07.10,00:00:07.77,Default,,0,0,0,,tenía algo.
Dialogue: 0,00:00:07.77,00:00:08.67,Default,,0,0,0,,Tesoros escondidos
Dialogue: 0,00:00:08.67,00:00:09.77,Default,,0,0,0,,a plena vista.
Dialogue: 0,00:00:09.77,00:00:10.20,Default,,0,0,0,,Y lo bonito
Dialogue: 0,00:00:10.20,00:00:10.97,Default,,0,0,0,,es que ahora
Dialogue: 0,00:00:10.97,00:00:11.47,Default,,0,0,0,,hay una forma
Dialogue: 0,00:00:11.47,00:00:12.10,Default,,0,0,0,,de hacer esto
Dialogue: 0,00:00:12.10,00:00:13.53,Default,,0,0,0,,aún más fácil.
Dialogue: 0,00:00:13.53,00:00:14.53,Default,,0,0,0,,Imagina que
Dialogue: 0,00:00:14.53,00:00:15.17,Default,,0,0,0,,antes de dejar
Dialogue: 0,00:00:15.17,00:00:16.27,Default,,0,0,0,,algo en la calle...
Dialogue: 0,00:00:16.27,00:00:16.97,Default,,0,0,0,,lo compartes
Dialogue: 0,00:00:16.97,00:00:17.70,Default,,0,0,0,,directamente
Dialogue: 0,00:00:17.70,00:00:18.57,Default,,0,0,0,,con tus vecinos.
Dialogue: 0,00:00:18.57,00:00:19.57,Default,,0,0,0,,Eso es Givore.
Dialogue: 0,00:00:19.57,00:00:19.97,Default,,0,0,0,,Foto,
Dialogue: 0,00:00:19.97,00:00:20.90,Default,,0,0,0,,ubicación,
Dialogue: 0,00:00:20.90,00:00:21.73,Default,,0,0,0,,publico.
Dialogue: 0,00:00:21.73,00:00:22.67,Default,,0,0,0,,Y alguien cerca
Dialogue: 0,00:00:22.67,00:00:22.93,Default,,0,0,0,,lo ve
Dialogue: 0,00:00:22.93,00:00:23.80,Default,,0,0,0,,y se lo lleva.
Dialogue: 0,00:00:23.80,00:00:24.70,Default,,0,0,0,,Sin esperas.
Dialogue: 0,00:00:24.70,00:00:25.30,Default,,0,0,0,,Sin depender
Dialogue: 0,00:00:25.30,00:00:26.40,Default,,0,0,0,,de la suerte.
Dialogue: 0,00:00:26.40,00:00:26.80,Default,,0,0,0,,Lo mejor
Dialogue: 0,00:00:26.80,00:00:27.07,Default,,0,0,0,,de todo
Dialogue: 0,00:00:27.07,00:00:28.13,Default,,0,0,0,,es que funciona
Dialogue: 0,00:00:28.13,00:00:28.50,Default,,0,0,0,,en las dos
Dialogue: 0,00:00:28.50,00:00:29.07,Default,,0,0,0,,direcciones.
Dialogue: 0,00:00:29.07,00:00:29.70,Default,,0,0,0,,Puedes encontrar
Dialogue: 0,00:00:29.70,00:00:30.97,Default,,0,0,0,,cosas increíbles...
Dialogue: 0,00:00:30.97,00:00:31.70,Default,,0,0,0,,y también puedes
Dialogue: 0,00:00:31.70,00:00:32.13,Default,,0,0,0,,compartir
Dialogue: 0,00:00:32.13,00:00:32.50,Default,,0,0,0,,lo que ya
Dialogue: 0,00:00:32.50,00:00:33.30,Default,,0,0,0,,no necesitas.
Dialogue: 0,00:00:33.30,00:00:34.30,Default,,0,0,0,,Vecinos conectando
Dialogue: 0,00:00:34.30,00:00:35.50,Default,,0,0,0,,sin conocerse.
Dialogue: 0,00:00:35.50,00:00:36.17,Default,,0,0,0,,Yo ya la llevo
Dialogue: 0,00:00:36.17,00:00:36.33,Default,,0,0,0,,siempre
Dialogue: 0,00:00:36.33,00:00:37.00,Default,,0,0,0,,en el móvil.
Dialogue: 0,00:00:37.00,00:00:37.60,Default,,0,0,0,,Nunca sabes
Dialogue: 0,00:00:37.60,00:00:37.97,Default,,0,0,0,,cuándo te va
Dialogue: 0,00:00:37.97,00:00:38.90,Default,,0,0,0,,a servir.
Dialogue: 0,00:00:38.90,00:00:39.27,Default,,0,0,0,,Y oye,
Dialogue: 0,00:00:39.27,00:00:39.60,Default,,0,0,0,,gracias
Dialogue: 0,00:00:39.60,00:00:40.07,Default,,0,0,0,,a los que ya
Dialogue: 0,00:00:40.07,00:00:40.50,Default,,0,0,0,,estáis usando
Dialogue: 0,00:00:40.50,00:00:40.93,Default,,0,0,0,,la app.
Dialogue: 0,00:00:40.93,00:00:41.37,Default,,0,0,0,,Cada cosa
Dialogue: 0,00:00:41.37,00:00:42.30,Default,,0,0,0,,que compartís
Dialogue: 0,00:00:42.30,00:00:42.100,Default,,0,0,0,,es una conexión
Dialogue: 0,00:00:43.00,00:00:43.53,Default,,0,0,0,,nueva.
Dialogue: 0,00:00:43.53,00:00:44.27,Default,,0,0,0,,Esto funciona
Dialogue: 0,00:00:44.27,00:00:45.50,Default,,0,0,0,,porque participáis.
Dialogue: 0,00:00:45.50,00:00:45.93,Default,,0,0,0,,Nos vemos
Dialogue: 0,00:00:45.93,00:00:46.77,Default,,0,0,0,,pedaleando.
